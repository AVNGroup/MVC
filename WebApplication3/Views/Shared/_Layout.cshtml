    <link href="~/js_css/css/StyleSheet2.css" rel="stylesheet" />
    <script type="text/javascript">
    var map;
    $(document).ready(function(){
      map = new GMaps({
        el: '#map',
        lat: 50.20,
        lng: 30.20,
        zoom:10,
        zoomControl : true,
        zoomControlOpt: {
            style : 'SMALL',
            position: 'TOP_LEFT'
        },
        panControl : false,
        streetViewControl : false,
        mapTypeControl: false,
        overviewMapControl: false
      });
  
      map.addMarker({
          lat: 50.27,
          lng: 30.30,
        title: 'Kyiv',
        details: {
          database_id: 42,
          author: 'HPNeo'
        },
        click: function(e){
          if(console.log)
            console.log(e);
          alert('You clicked in this marker');
        },
        mouseover: function(e){
          if(console.log)
            console.log(e);
        }
      });
      map.addMarker({
          lat: 43.13,
        lng: 27.55,
        title: 'Varna',
        infoWindow: {
          content: '<p>HTML Content</p>'
        }
      });
    });
    </script>

